main.c
#include <stdio.h>

/* Algo1: O(n) */
void Algo1(int n)
{
    for (int i = 1; i <= n; i++)
    {
        printf("Hello World (Algo1)\n");
    }
}

/* Algo2: O(n^2) */
void Algo2(int n)
{
    for (int i = 1; i <= n; i++)
    {
        for (int j = 1; j <= n; j++)
        {
            printf("Hello World (Algo2)\n");
        }
    }
}

/* Algo3: O(log₂ n)  -> i = i * 2 */
void Algo3(int n)
{
    for (int i = 1; i < n; i = i * 2)
    {
        printf("Hello World (Algo3)\n");
    }
}

/* Algo4: O(log₅ n)  -> i = i / 5 */
void Algo4(int n)
{
    for (int i = n; i > 1; i = i / 5)
    {
        printf("Hello World (Algo4)\n");
    }
}

/* Algo5: O(log₅ n³) -> i = i * 5 */
void Algo5(int n)
{
    int limit = n * n * n;
    for (int i = 1; i < limit; i = i * 5)
    {
        printf("Hello World (Algo5)\n");
    }
}

/* Algo6: O(√n) -> i^2 <= n */
void Algo6(int n)
{
    for (int i = 1; i * i <= n; i++)
    {
        printf("Hello World (Algo6)\n");
    }
}

/* Algo7: O(√n) -> artan toplama serisi */
void Algo7(int n)
{
    int i = 1;
    int k = 1;
    while (k <= n)
    {
        printf("Hello World (Algo7)\n");
        i = i + 1;
        k = k + i;  // 1, 3, 6, 10, 15...
    }
}

int main()
{
    int n = 20;

    Algo1(n);
    Algo2(n);
    Algo3(n);
    Algo4(n);
    Algo5(n);
    Algo6(n);
    Algo7(n);

    return 0;
}
